xt-decoration: none;\n    font-weight: 500;\n    gap: 0.25rem;\n    letter-spacing: 0;\n    line-height: 1;\n    padding: 0.75rem 1.2rem;\n    white-space: nowrap;\n}\na.button:hover, button:hover {\n    border-color: #666;\n    color:white;\n}\n@media screen and (min-width: 1060px) {\n  .project-preview {\n    grid-template-columns: repeat(3, 1fr);\n  }\n}\n@media screen and (min-width: 700px) {\n  .project-preview {\n    grid-template-columns: repeat(2, 1fr);\n  }\n  .stars {\n    right: 1.5rem;\n    top: 1.5rem;\n  }\n  .card.anchored {\n    padding-bottom: 3.5rem;\n  }\n  .card.anchored.large {\n    padding-bottom: 5rem;\n  }\n  .card {\n    padding: 1.5rem;\n  }\n  .card a.card-header,\n  .card h2 {\n    font-size: 1.1rem;\n    font-weight: 500;\n  }\n  .links.anchored {\n    bottom: 1.5rem;\n    left: 1.5rem;\n    position: absolute;\n}\n.links {\n    margin-top: 1rem;\n}\n}\n@media screen and (max-width: 900px) {\n  .home-wrapper {\n    background-color: rgb(24, 24, 24);\n    padding: 20px 0px;\n  }\n}\n</style>\n"],"mappings":"AAgFA;AACA,OAAAA,MAAA;AACA,OAAAC,aAAA;AACA,OAAAC,YAAA;AACA;EACAC,IAAA;EACAC,UAAA;IACAJ,MAAA;IACA;IACAC;EACA;EACAI,KAAA;IACA;MACAC,QAAA,EAAAJ;IACA;EACA;AACA"},"metadata":{},"sourceType":"module","externalDependencies":[]}